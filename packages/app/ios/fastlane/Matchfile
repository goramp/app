git_url(ENV['FASTLANE_GIT_URL'])

storage_mode("git")

type("appstore") # The default type, can be: appstore, adhoc, enterprise or development

app_identifier([ENV['FASTLANE_APP_ID']])
username(ENV['FASTLANE_USER'])

